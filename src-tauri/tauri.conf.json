{
  "productName": "Tautify",
  "version": "0.1.1",
  "identifier": "com.lemon2ee.tautify",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-spotify-v2",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "titleBarStyle": "Transparent",
        "hiddenTitle": true
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "plugins": {
    "deep-link": {
      "mobile": [],
      "desktop": {
        "schemes": [
          "taurispotify"
        ]
      }
    }
  }
}
